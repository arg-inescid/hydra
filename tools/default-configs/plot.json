{
	"plot": "plot1",
	"start_server":
	{
		"port": 8080,
		"img_width": 640,
		"img_height": 360
	},
	"plots": [
		{
			"type": "latency",
            "output": "../benchmarks/results/hello-world-java/hello-world-latency.png",
			"show_image": false,
			"data":
			[
				{
					"title": "Hotspot w agent",
					"filepath": "../benchmarks/results/hello-world-java/hello-world-java-1.dat",
					"regex": "99%\\s*(\\d*)"
				},
				{
					"title": "Hotspot",
					"filepath": "../benchmarks/results/hello-world-java/hello-world-java-2.dat",
					"regex": "99%\\s*(\\d*)"
				},
				{
					"title": "VMM",
					"filepath": "../benchmarks/results/hello-world-java/hello-world-java-3.dat",
					"regex": "99%\\s*(\\d*)"
				}
			]
		},
		{
			"type": "throughput",
            "output": "../benchmarks/results/hello-world-java/hello-world-throughput.png",
			"show_image": false,
			"data":
			[
				{
					"title": "Hotspot w agent",
					"filepath": "../benchmarks/results/hello-world-java/hello-world-java-1.dat",
					"regex": "Requests per second:\\s*(\\d*)"
				},
				{
					"title": "Hotspot",
					"filepath": "../benchmarks/results/hello-world-java/hello-world-java-2.dat",
					"regex": "Requests per second:\\s*(\\d*)"
				},
				{
					"title": "VMM",
					"filepath": "../benchmarks/results/hello-world-java/hello-world-java-3.dat",
					"regex": "Requests per second:\\s*(\\d*)"
				}
			]
		},
		{
			"type": "footprint",
            "output": "../benchmarks/results/hello-world-java/hello-world-footprint.png",
			"show_image": false,
			"data":
			[
				{
					"title": "VMM footprint",
					"regex": "Maximum resident set size \\(kbytes\\):\\s*(\\d*)"
				}
			]
		},
		{
			"type": "startup",
            "output": "../benchmarks/results/hello-world-java/hello-world-startup.png",
			"show_image": false,
			"data":
			[
				{
					"title": "VMM startup",
					"regex": "VMM boot time:\\s*(\\d*)"
				}
			]
		},
		{
			"type": "scalability",
            "output": "../benchmarks/results/hello-world-java/hello-world-scalability.png",
			"show_image": false,
			"data":
			[
				{
					"title": "System scalability"
				}
			]
		},
		{
			"type": "total_memory",
            "output": "../benchmarks/results/hello-world-java/hello-world-total-memory.png",
			"show_image": false,
			"data":
			[
				{
					"title": "Total system memory",
					"regex": "Maximum resident set size \\(kbytes\\):\\s*(\\d*)"
				}
			]
		}
	]
}